N		len = parse_name(msg, cp, name, MAXCNAME);
X		len = parse_name(msg, cp, name, (32 + (32>>1)));
N		if (len == -1) return 0;
N
N		cp += len;
N		break;
N	case TYPE_SOA:
X	case 6:
N		/* Get domain name of name server */
N		len = parse_name(msg, cp, name, MAXCNAME);
X		len = parse_name(msg, cp, name, (32 + (32>>1)));
N		if (len == -1) return 0;
N
N		cp += len;
N
N		/* Get domain name of responsible person */
N		len = parse_name(msg, cp, name, MAXCNAME);
X		len = parse_name(msg, cp, name, (32 + (32>>1)));
N		if (len == -1) return 0;
N
N		cp += len;
N
N		cp += 4;
N		cp += 4;
N		cp += 4;
N		cp += 4;
N		cp += 4;
N		break;
N	case TYPE_TXT:
X	case 16:
N		/* Just stash */
N		break;
N	default:
N		/* Ignore */
N		break;
N	}
N
N	return cp;
